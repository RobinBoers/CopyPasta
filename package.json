{
	"name": "copypasta",
	"displayName": "CopyPasta",
	"description": "We all know that you don't even look at the questions in Stack Overflow. Why wouldn't you just copypasta the first comments code section?",
	"version": "0.1.0",
	"publisher": "makman12",
	"icon": "copypasta.png",
	"repository": {
		"url": "https://github.com/makman12/CopyPasta.git"
	},
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:copypasta.copypasta"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "copypasta.copypasta",
				"title": "Copy Pasta"
			}
		],
		"keybindings": [
			{
				"command": "copypasta.copypasta",
				"key": "ctrl+enter",
				"mac": "cmd+enter",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"request": "^2.34",
		"cheerio": "^1.0.0-rc.10",
		"clipboardy": "^2.3.0",
		"request-promise": "^4.2.6"
	}
}
